{
  "name": "designlibre",
  "version": "0.1.0",
  "type": "module",
  "description": "Distributed, GPU-accelerated vector CAD system with CRDT-backed scene graph",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build:fast": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:visual": "vitest run --config tests/visual/vitest.config.ts",
    "test:visual:watch": "vitest --config tests/visual/vitest.config.ts",
    "test:visual:update": "vitest run --config tests/visual/vitest.config.ts --update-snapshots",
    "test:visual:report": "node tests/visual/utils/report-generator.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:icons": "tauri icon"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-http": "^2.5.4",
    "@types/jszip": "^3.4.1",
    "ajv": "^8.17.1",
    "harfbuzzjs": "^0.3.6",
    "jszip": "^3.10.1",
    "kiwi.js": "^1.1.3",
    "quickjs-emscripten": "^0.31.0",
    "y-protocols": "^1.0.7",
    "yjs": "^13.6.29"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.82",
    "@playwright/test": "^1.40.0",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^20.10.0",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@unocss/preset-attributify": "^66.5.12",
    "@unocss/preset-icons": "^66.5.12",
    "@unocss/preset-uno": "^66.5.12",
    "@unocss/transformer-directives": "^66.5.12",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.2.0",
    "puppeteer": "^21.0.0",
    "sharp": "^0.33.0",
    "typescript": "^5.4.0",
    "unocss": "^66.5.12",
    "vite": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
