{
  "name": "colors-solid",
  "description": "Solid color fills",
  "category": "styling",
  "platforms": ["react", "css"],
  "canvas": {
    "width": 400,
    "height": 200,
    "backgroundColor": "#FFFFFF"
  },
  "root": {
    "type": "FRAME",
    "name": "ColorPalette",
    "width": 360,
    "height": 160,
    "x": 20,
    "y": 20,
    "autoLayout": {
      "mode": "HORIZONTAL",
      "itemSpacing": 16,
      "padding": 16
    },
    "children": [
      {
        "type": "RECTANGLE",
        "name": "Red",
        "width": 50,
        "height": 128,
        "fills": [{ "type": "SOLID", "color": { "r": 0.91, "g": 0.3, "b": 0.24 } }],
        "cornerRadius": 8
      },
      {
        "type": "RECTANGLE",
        "name": "Green",
        "width": 50,
        "height": 128,
        "fills": [{ "type": "SOLID", "color": { "r": 0.18, "g": 0.8, "b": 0.44 } }],
        "cornerRadius": 8
      },
      {
        "type": "RECTANGLE",
        "name": "Blue",
        "width": 50,
        "height": 128,
        "fills": [{ "type": "SOLID", "color": { "r": 0.2, "g": 0.6, "b": 0.86 } }],
        "cornerRadius": 8
      },
      {
        "type": "RECTANGLE",
        "name": "Purple",
        "width": 50,
        "height": 128,
        "fills": [{ "type": "SOLID", "color": { "r": 0.56, "g": 0.27, "b": 0.68 } }],
        "cornerRadius": 8
      },
      {
        "type": "RECTANGLE",
        "name": "Orange",
        "width": 50,
        "height": 128,
        "fills": [{ "type": "SOLID", "color": { "r": 0.95, "g": 0.61, "b": 0.07 } }],
        "cornerRadius": 8
      }
    ]
  }
}
