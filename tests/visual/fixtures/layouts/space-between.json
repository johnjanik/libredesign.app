{
  "name": "space-between",
  "description": "Horizontal layout with space-between alignment",
  "category": "layout",
  "platforms": ["react", "css"],
  "canvas": {
    "width": 400,
    "height": 150,
    "backgroundColor": "#FFFFFF"
  },
  "root": {
    "type": "FRAME",
    "name": "Container",
    "width": 360,
    "height": 110,
    "x": 20,
    "y": 20,
    "fills": [
      {
        "type": "SOLID",
        "color": { "r": 0.95, "g": 0.95, "b": 0.95 }
      }
    ],
    "cornerRadius": 8,
    "autoLayout": {
      "mode": "HORIZONTAL",
      "itemSpacing": 0,
      "padding": 16,
      "primaryAxisAlign": "SPACE_BETWEEN",
      "counterAxisAlign": "CENTER"
    },
    "children": [
      {
        "type": "RECTANGLE",
        "name": "Left",
        "width": 60,
        "height": 60,
        "fills": [
          {
            "type": "SOLID",
            "color": { "r": 0.56, "g": 0.27, "b": 0.68 }
          }
        ],
        "cornerRadius": 4
      },
      {
        "type": "RECTANGLE",
        "name": "Center",
        "width": 60,
        "height": 60,
        "fills": [
          {
            "type": "SOLID",
            "color": { "r": 0.95, "g": 0.61, "b": 0.07 }
          }
        ],
        "cornerRadius": 4
      },
      {
        "type": "RECTANGLE",
        "name": "Right",
        "width": 60,
        "height": 60,
        "fills": [
          {
            "type": "SOLID",
            "color": { "r": 0.1, "g": 0.74, "b": 0.61 }
          }
        ],
        "cornerRadius": 4
      }
    ]
  }
}
