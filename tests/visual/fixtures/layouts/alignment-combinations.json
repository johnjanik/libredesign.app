{
  "name": "alignment-combinations",
  "description": "Grid of alignment combinations: justify x align",
  "category": "layout",
  "platforms": ["react", "css"],
  "canvas": {
    "width": 500,
    "height": 500,
    "backgroundColor": "#FFFFFF"
  },
  "root": {
    "type": "FRAME",
    "name": "AlignmentGrid",
    "width": 460,
    "height": 460,
    "x": 20,
    "y": 20,
    "autoLayout": {
      "mode": "VERTICAL",
      "itemSpacing": 20,
      "padding": 20
    },
    "children": [
      {
        "type": "FRAME",
        "name": "Row1-MIN",
        "width": 420,
        "height": 100,
        "fills": [{ "type": "SOLID", "color": { "r": 0.95, "g": 0.95, "b": 0.95 } }],
        "cornerRadius": 8,
        "autoLayout": {
          "mode": "HORIZONTAL",
          "itemSpacing": 20,
          "padding": 10,
          "primaryAxisAlign": "MIN",
          "counterAxisAlign": "MIN"
        },
        "children": [
          { "type": "RECTANGLE", "width": 40, "height": 40, "fills": [{ "type": "SOLID", "color": { "r": 0.91, "g": 0.3, "b": 0.24 } }] },
          { "type": "RECTANGLE", "width": 40, "height": 60, "fills": [{ "type": "SOLID", "color": { "r": 0.18, "g": 0.8, "b": 0.44 } }] },
