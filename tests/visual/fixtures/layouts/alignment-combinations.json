          { "type": "RECTANGLE", "width": 40, "height": 40, "fills": [{ "type": "SOLID", "color": { "r": 0.56, "g": 0.27, "b": 0.68 } }] },
          { "type": "RECTANGLE", "width": 40, "height": 60, "fills": [{ "type": "SOLID", "color": { "r": 0.95, "g": 0.61, "b": 0.07 } }] },
