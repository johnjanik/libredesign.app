        "width": 420,
        "height": 100,
        "fills": [{ "type": "SOLID", "color": { "r": 0.95, "g": 0.95, "b": 0.95 } }],
        "cornerRadius": 8,
        "autoLayout": {
          "mode": "HORIZONTAL",
          "itemSpacing": 20,
          "padding": 10,
          "primaryAxisAlign": "CENTER",
          "counterAxisAlign": "CENTER"
